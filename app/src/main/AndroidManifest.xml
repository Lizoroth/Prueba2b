<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.franco.prueba2b">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Principal" />
        <activity android:name=".Registro"
            android:parentActivityName="com.example.franco.prueba2b.Menu"/>
        <activity android:name=".Listado"
            android:parentActivityName="com.example.franco.prueba2b.Menu" />
        <activity android:name=".Modificar"
            android:parentActivityName="com.example.franco.prueba2b.Listado" />
        <activity android:name=".Menu"
            android:parentActivityName="com.example.franco.prueba2b.Principal" />
        <activity android:name=".Clima"
            android:parentActivityName="com.example.franco.prueba2b.Menu"/>
        <activity android:name=".Acerca"
            android:parentActivityName="com.example.franco.prueba2b.Menu">
        </activity>
    </application>

</manifest>